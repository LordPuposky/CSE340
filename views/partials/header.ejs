<header>
    <div class="logo">
        <a href="/">CSE Motors</a>
    </div>
    <nav>
        <% if (locals.loggedin) { %>
            <a href="/account/" title="Welcome and Account Management">
                WELCOME <%= locals.accountData.account_firstname.toUpperCase() %>
            </a>
            <a href="/account/logout" title="Click to log out">
                LOGOUT
            </a>
        <% } else { %>
            <a href="/account/login" class="my-account" title="Click to log in or register">
                My Account
            </a>
        <% } %>
    </nav>
</header>